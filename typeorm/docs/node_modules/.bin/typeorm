#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jordantanaliga100/Personal/ormCore/typeorm/docs/node_modules/.pnpm/typeorm@0.3.27_mysql2@3.15.2_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.7.1_typescript@5.9.3_/node_modules/typeorm/node_modules:/home/jordantanaliga100/Personal/ormCore/typeorm/docs/node_modules/.pnpm/typeorm@0.3.27_mysql2@3.15.2_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.7.1_typescript@5.9.3_/node_modules:/home/jordantanaliga100/Personal/ormCore/typeorm/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jordantanaliga100/Personal/ormCore/typeorm/docs/node_modules/.pnpm/typeorm@0.3.27_mysql2@3.15.2_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.7.1_typescript@5.9.3_/node_modules/typeorm/node_modules:/home/jordantanaliga100/Personal/ormCore/typeorm/docs/node_modules/.pnpm/typeorm@0.3.27_mysql2@3.15.2_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.7.1_typescript@5.9.3_/node_modules:/home/jordantanaliga100/Personal/ormCore/typeorm/docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/typeorm@0.3.27_mysql2@3.15.2_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.7.1_typescript@5.9.3_/node_modules/typeorm/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/typeorm@0.3.27_mysql2@3.15.2_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.7.1_typescript@5.9.3_/node_modules/typeorm/cli.js" "$@"
fi
